---
title: search
layout: none
search: exclude
---
[
  {% for page in site.pages %}
  {% unless page.search == "exclude" %}
    {
      "title"    : "{{ page.title | escape }}",
      "category" : "{{ page.category }}",
      "tags"     : "{{ page.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ page.url }}",
      "date"     : "{{ page.date }}",
      "content": {{ page.content | jsonify }}
    },
{% endunless %}
  {% endfor %}
]
