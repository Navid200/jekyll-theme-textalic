---
layout: null
---
[
  {% for post in site.pages %}
    {
      "title"    : "{{ page.title | escape }}",
      "category" : "{{ page.category }}",
      "tags"     : "{{ page.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ page.url }}",
      "date"     : "{{ page.date }}",
      "content": {{ page.content | jsonify }}
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
